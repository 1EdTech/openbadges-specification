{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "More Info Link",
  "description": "An extension allowing for the addition of the geographic location where an individual earned a badge.",
  "type": "object",
  "properties": {
    "description": {
      "type": "string",
    },
    "elevation": {
      "type": "string",
    },
    "latitude": {
      "type": "string",
    },
    "latitude": {
      "type": "string",
    },
    "required": ["description", "latitude","longitude"],
}
